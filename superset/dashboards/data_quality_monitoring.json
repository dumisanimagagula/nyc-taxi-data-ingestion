{
  "dashboard_title": "Data Quality Monitoring",
  "description": "Comprehensive data quality metrics across Bronze, Silver, and Gold layers",
  "slug": "data-quality-monitoring",
  "position_json": "{\"DASHBOARD_VERSION_KEY\": \"v2\"}",
